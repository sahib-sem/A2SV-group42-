n,k = list(map(int,input().split()))
lst=list(map(int,input().split()))
lst.sort()
if k==0:
    if lst[0]!=1:
        print(1)
    else:
        print(-1)
else:

    if k==len(lst):
        print(lst[-1])
    else:
        first=lst[k-1]
        second=lst[k]
        if second>first:
            print(first)
        else:
            print(-1)
